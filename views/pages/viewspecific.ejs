<!DOCTYPE html>

 <html lang="en">
 <head>
   <meta charset="utf-8">
   <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css" rel="stylesheet">
   <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"> </script>
 <script type="text/javascript" src="index.js"></script>
 <style>
 p{
   text-transform:capitalize;
   font-family:'Times New Roman',Times,serif;
 }
 #tableContainer{
   padding-top: 10px;
}
table{
 width: 300px;
 margin: 0 auto;
 font-family: Tahoma, Geneva, sans-serif;
   text-align: center;
   padding: auto;
   padding-top: 100px;
}
th {
 background-color: #F5F5DC;
 color: black;
}
tr:nth-child(even) {background-color: #f2f2f2;}
table, th, td{
   border: 1px solid grey;
   padding: 20px;
}
a:link {
 color: red;

}
a:visited {
 color: green;
}
a:hover {
 color: hotpink;
}
a:active {
 color: blue;
}
body{
 background-color: gainsboro;
 font-family: Calibri, sans-serif;
}

 </style>

 </head>
 <body>
     <header>
          <h1 align="center"style="font-size:30px;background-color:#F5F5DC;position:float">DATABASE</h1>
     </header>
     <div class = "content" id = "tableid">
         <table>

             <tr>
                 <th> First name </th>
                 <th>Last Name</th>
                 <th>Username</th>
                 <th>Email</th>
                 <th>Phone number</th>
                 <th>Type</th>
                 <th>Password</th>
             </tr>
            <% rows.forEach(function(r){ %>
                <tr>

                    <td ><%= r.fname%></td>
                      <td ><%= r.lname%></td>
                        <td ><%= r.username%></td>
                          <td ><%= r.email%></td>
                            <td ><%= r.phone%></td>
                            <td ><%= r.type%></td>
                              <td ><%= r.password%></td>
                 </tr>
             <% }); %>
         </table>

     </div>






</body>
</html>
