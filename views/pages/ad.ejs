<!DOCTYPE html>

<html lang="en">
<head>
  <meta charset="utf-8">

  <title> DATABASE</title>
  <link rel="stylesheet" href="./stylesheets/style.css">


</head>
<body>
    <header>
          <h1 align="center"style="font-size:30px;background-color:#F5F5DC;position:float">DATABASE</h1>
    </header>
    <nav >
        <ul>
            <li>
                <a class="nav-link" href="/project.html">update</a>
            </li>
        </ul>
    </nav>
    <div class = "content" id = "tableid">
        <table>

            <tr>
                <th>First name</th>
                <th>Last Name</th>
                <th>Username</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Type</th>
                <th>Password</th>
            </tr>
            <% rows.forEach(function(r) { %>
                <tr>
                    <td contenteditable="true"><%= r.fname%></td>
                      <td contenteditable="true"><%= r.lname%></td>
                        <td contenteditable="true"><%= r.username%></td>
                          <td contenteditable="true"><%= r.email%></td>
                            <td contenteditable="true"><%= r.phone%></td>
                              <td contenteditable="true"><%= r.type%></td>
                                <td contenteditable="true"><%= r.password%></td>
                </tr>
            <% }); %>
        </table>

    </div>

</body>
</html>
